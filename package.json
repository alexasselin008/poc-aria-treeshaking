{
    "name": "@sandbox/app",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Application to try the web configs.",
    "private": true,
    "license": "Apache-2.0",
    "type": "module",
    "msw": {
        "workerDirectory": "public"
    },
    "scripts": {
        "dev": "nodemon",
        "dev-msw": "cross-env USE_MSW=true nodemon",
        "dev-verbose": "cross-env VERBOSE=true nodemon",
        "dev-msw-verbose": "cross-env USE_MSW=true VERBOSE=true nodemon",
        "typecheck": "tsc --noEmit",
        "build": "webpack --config webpack.build.js",
        "serve-build": "pnpm build && pnpm http-server dist -p 8080 -P http://localhost:8080? -c-1",
        "build-verbose": "cross-env PROFILE=true webpack --config webpack.build.js"
    },
    "devDependencies": {
        "@svgr/webpack": "8.1.0",
        "@swc/core": "1.3.85",
        "@swc/helpers": "0.5.2",
        "@swc/jest": "0.2.29",
        "@testing-library/react": "14.0.0",
        "@types/jest": "29.5.5",
        "@types/node": "20.6.2",
        "@types/react": "18.2.21",
        "@types/react-dom": "18.2.7",
        "@workleap/browserslist-config": "2.0.0",
        "@workleap/eslint-plugin": "3.0.0",
        "@workleap/postcss-configs": "1.0.1",
        "@workleap/stylelint-configs": "2.0.0",
        "@workleap/swc-configs": "2.1.2",
        "@workleap/typescript-configs": "3.0.2",
        "@workleap/webpack-configs": "1.2.1",
        "browserslist": "4.21.10",
        "cross-env": "7.0.3",
        "http-server": "14.1.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "msw": "1.3.1",
        "nodemon": "3.0.1",
        "postcss": "8.4.29",
        "postcss-preset-env": "9.1.3",
        "ts-jest": "29.1.1",
        "typescript": "5.2.2",
        "webpack": "5.88.2",
        "webpack-cli": "5.1.4",
        "webpack-dev-server": "4.15.1",
        "webpack-bundle-analyzer": "4.10.1",
        "@types/webpack-bundle-analyzer": "4.4.0"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.20.1",
        "@hopper-ui/icons": "1.0.1",
        "@hopper-ui/styled-system": "0.2.2",
        "react-aria-components": "1.0.0-beta.1"
    }
}
